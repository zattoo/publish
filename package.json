        "changelog-parser": "3.0.1",
        "fs-extra": "11.2.0",
        "glob": "10.3.10",
        "npm-registry-fetch": "16.1.0"
    },
    "devDependencies": {
        "@types/changelog-parser": "2.8.4",
        "@types/fs-extra": "11.0.4",
