{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext"],
      "module": "NodeNext",
      "baseUrl": "./src",
      "types": [
        "node",
      ],
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*",],
  }
